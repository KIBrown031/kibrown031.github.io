    "version": "0.2.0",
    "configurations": [             

        {
            "name": "Live Server",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:5501",
            "webRoot": "${workspaceFolder}"
        },


        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/promise.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },

        
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "https://keithibrown.org/mysite.index.html",
            "webRoot": "${workspaceFolder}"
        },
             

       
    ]
}